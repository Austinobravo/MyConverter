{
    "functions": {
      "api/test.js": {
        "memory": 3008,
        "maxDuration": 30
      },
      "api/*.js": {
        "memory": 3008,
        "maxDuration": 30
      }
    },
    "builds": [{
        "src": "Converter/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Converter/wsgi.py"
        }
    ]
}
